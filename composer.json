{
    "name": "dlayer/dlayer-v2",
    "description": "Dlayer is an Open Source responsive web development tool aimed primarily at users with limited web design or development experience.",
    "type": "project",
    "license": "MIT",
    "keywords": [
        "website-builder",
        "dlayer",
        "form-builder"
    ],
    "homepage": "http://www.dlayer.com/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-component-installer": "^1.0 || ^0.3 || ^1.0.0-dev@dev",
        "zendframework/zend-mvc": "^3.0.1",
        "zfcampus/zf-development-mode": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/",
            "Install\\": "module/Install/src/"
        }
    },
    "autoload-dev": {
    },
    "extra": [],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php",
        "test": "phpunit"
    }
}
